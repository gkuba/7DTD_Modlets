<KatalaSwiftwind>

<!-- Empty Pallet -->
<set xpath="/blocks/block[@name='palletEmpty']">
	<property name="Group" value="Decor,Commercial,Blocks,Player"/>
	<property name="Class" value="ULM_Decor,UndeadLegacy"/>
	<property name="Extends" value="woodWeakNoUpgradeMaster" param1="Group"/>
	<property name="Shape" value="ModelEntity"/><property name="Model" value="Entities/Industrial/palletPrefab"/>
	<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
	<property name="DescriptionKey" value="creativeMenuBlockGroupDesc"/>
	<property name="EconomicValue" value="4"/><property name="CarryWeight" value="0.5"/><property name="Weight" value="50"/>
	<drop event="Harvest" name="ulmResourcePlank" count="2,3" tag="allHarvest"/>
	<drop event="Harvest" name="ulmResourceScrapPlank" count="1,2" tag="allHarvest"/>
	<drop event="Harvest" name="resourceNail" count="2,4" tag="allHarvest"/>
	<drop event="Destroy" name="ulmResourceScrapWood" count="1,2"/></set>
	
